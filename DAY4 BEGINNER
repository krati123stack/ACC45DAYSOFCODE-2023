#include <iostream>

using namespace std;

int main() {
    int T;
    cin >> T; // Number of test cases

    while (T--) {
        int N;
        cin >> N; // Number of friends

        // Calculate the minimum number of cars required
        int cars_required = (N + 3) / 4;

        cout << cars_required << endl;
    }

    return 0;
}
