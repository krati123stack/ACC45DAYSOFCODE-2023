#include <iostream>

using namespace std;

int main() {
    int T; // Number of test cases
    cin >> T;

    while (T--) {
        int N; // Number of hoops
        cin >> N;

        if (N == 1) {
            cout << "1" << endl;
        } else {
            cout << "2" << endl;
        }
    }

    return 0;
}
