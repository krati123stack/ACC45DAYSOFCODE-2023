#include <iostream>

using namespace std;

int main() {
    int T;
    cin >> T; // Number of test cases

    while (T--) {
        int X, Y;
        cin >> X >> Y; // Following and follower count

        // Check if the account is spam or not
        if (X <= 10 * Y) {
            cout << "NO" << endl;
        } else {
            cout << "YES" << endl;
        }
    }

    return 0;
}
